<view class="container"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}"><block wx:for="{{imgUrls}}" wx:for-item="img" wx:for-index="index" wx:key="index"><swiper-item><image class="slide-image" src="{{img.url}}" mode="aspectFill"></image></swiper-item></block></swiper><view data-event-opts="{{[['tap',[['getInfo',['$event']]]]]}}" bindtap="__e">点击</view><text>{{text}}</text><view data-event-opts="{{[['tap',[['getCity',['$event']]]]]}}" bindtap="__e">北京</view><button class="bottom" type="primary" open-type="getUserInfo" withCredentials="true" lang="zh_CN" data-event-opts="{{[['getuserinfo',[['wxGetUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">授权登录</button></view>